system:
  host: 0.0.0.0
  port: 16181
  http_client_timeout: 5
  web_ui: true
  detail_concurrency: 5
captcha:
  enable: true
  save_failed_img: false
  save_failed_img_path: faile_captcha
  device:
  - CPU
  retry_times: 10
  coding_code: auto
  coding_show: false
proxy:
  local_ipv6_pool:
    enable: false
    pool_num: 88
    check_interval: 1
    ipv6_network_card: eth0
  tunnel:
    url: null
  extra_api:
    url: null
    extra_interval: 3
    timeout: 100
    timeout_drop: 8
    check_proxy: true
    proxy_timeout: 0.5
    check_proxy_num: 20
    auto_maintenace: true
    pool_num: 100
risk_avoidance:
  allow_type:
  - web
  - app
  - mapp
  - kapp
  - bweb
  - bapp
  - bmapp
  - bkapp
  prohibit_suffix:
  - .gov.cn
log:
  dir: logs
  file_head: ymicp
  backup_count: 7
  save_log: false
  output_console: true
